<Window x:Class="Client.View.Session.WindowLogIn"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client.View.Session"
        mc:Ignorable="d"
        Title="WindowLogIn" Height="500" Width="400">
    <Grid Background="#FFC30F45">
        <Grid.RowDefinitions>
            <RowDefinition Height="171*"/>
            <RowDefinition Height="71*"/>
        </Grid.RowDefinitions>
        <Label Content="Log In" Margin="0,42,0,0" VerticalAlignment="Top" FontSize="48" FontWeight="Bold" FontStyle="Italic" FontFamily="Yu Gothic" HorizontalAlignment="Center" Width="184" Background="{x:Null}" Foreground="White"/>
        <Rectangle HorizontalAlignment="Center" Height="275" Margin="0,166,0,0" Stroke="Black" VerticalAlignment="Top" Width="326" Fill="#FF231123" RadiusX="25.8" RadiusY="25.8" Grid.RowSpan="2"/>
        <TextBox x:Name="TextBoxEmailInput" HorizontalAlignment="Left" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="174" FontSize="16" Background="White" Height="29" Margin="108,238,0,0"/>
        <Button Content="Log In" HorizontalAlignment="Left" Margin="108,20,0,0" VerticalAlignment="Top" Background="#FFC30F45" Foreground="White" BorderBrush="{x:Null}" FontFamily="Yu Gothic" Width="174" FontSize="16" Click="ButtonLogIn" Grid.Row="1"/>
        <Label Content="Email" HorizontalAlignment="Left" Margin="108,207,0,0" VerticalAlignment="Top" Background="{x:Null}" Foreground="White"/>
        <Label Content="Password" HorizontalAlignment="Left" Margin="108,272,0,0" VerticalAlignment="Top" Foreground="White"/>
        <PasswordBox x:Name="PasswordBoxPasswordInput" HorizontalAlignment="Left" Margin="108,303,0,0" VerticalAlignment="Top" Width="174" Height="29"/>
    </Grid>
</Window>
